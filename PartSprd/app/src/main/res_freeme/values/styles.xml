<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="PanelIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@null</item>
    </style>

    <style name="Theme.Freeme.DayNight" parent="@*freeme:style/Theme.Freeme.Light"/>

    <style name="Theme.Freeme.NoTitleBar.Fullscreen.DayNight" parent="Theme.Freeme.DayNight">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="DreamSeekBarExceptDepthBlurModule" parent="DreamSeekbar">
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingEnd">10dp</item>
    </style>
</resources>
